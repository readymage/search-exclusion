    "type": "magento2-module",
    "version": "1.0.0",
    "license": "proprietary",
    "authors": [
        {
            "name": "Scandiweb / Readymage",
            "homepage": "https://www.scandiweb.com/"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "magento/framework": "*",
        "magento/module-store": "*",
        "magento/module-theme": "*"
    },
    "autoload": {
        "files": [
            "src/registration.php"
        ],
        "psr-4": {
            "ReadyMage\\SearchExclusion\\": "src/"
        }
    }
}
